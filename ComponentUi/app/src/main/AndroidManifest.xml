<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zzxb.me.componentui">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RadioActivity" />
        <activity android:name=".CheckActivity" />
        <activity android:name=".ToggleActivity" />
        <activity android:name=".ImageViewActivity" />
        <activity android:name=".SpinnerActivity" />
        <activity android:name=".AutoTextActivity" />
        <activity android:name=".ProgressActivity" />
        <activity android:name=".SeekActivity" />
        <activity android:name=".RatingActivity" />
        <activity android:name=".TabHostActivity" />
        <activity android:name=".AlertActivity" />
        <activity android:name=".ImageButtonActivity"></activity>
    </application>

</manifest>